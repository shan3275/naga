LIB ?= lib$(PROG).a

include $(MKDIR)/make.var

CFLAGS  += -DMAKELIB -g -Wall -I$(SRCDIR)/../boots  -I$(SRCDIR)/include -I$(SRCDIR)/libzebra

all: $(LIB)
%.c: 

%.o: %.c
	$(CC) $(CFLAGS) $(EFLAGS) -c $< -o $@ 


$(LIB): $(OBJS)
	$(AR) -rcs $@ $^

clean: $(VCLEAN)
	rm -rf *.o
	rm -rf *.a
	rm -rf $(PROG)

